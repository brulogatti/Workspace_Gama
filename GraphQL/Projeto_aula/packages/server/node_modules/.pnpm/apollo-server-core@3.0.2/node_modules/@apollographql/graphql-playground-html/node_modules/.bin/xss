    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/xss@1.0.9/node_modules/xss/bin/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/xss@1.0.9/node_modules/xss/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/xss@1.0.9/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules:/mnt/c/Workspace - Gama/GraphQL/node_modules:/mnt/c/Workspace - Gama/node_modules:/mnt/c/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/@apollographql+graphql-playground-html@1.6.29/node_modules/xss/bin/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/@apollographql+graphql-playground-html@1.6.29/node_modules/xss/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/@apollographql+graphql-playground-html@1.6.29/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/@apollographql+graphql-playground-html@1.6.29/node_modules/@apollographql/graphql-playground-html/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/@apollographql+graphql-playground-html@1.6.29/node_modules/@apollographql/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/xss@1.0.9/node_modules/xss/bin/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/xss@1.0.9/node_modules/xss/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/xss@1.0.9/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules:/mnt/c/Workspace - Gama/GraphQL/node_modules:/mnt/c/Workspace - Gama/node_modules:/mnt/c/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/@apollographql+graphql-playground-html@1.6.29/node_modules/xss/bin/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/@apollographql+graphql-playground-html@1.6.29/node_modules/xss/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/@apollographql+graphql-playground-html@1.6.29/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/@apollographql+graphql-playground-html@1.6.29/node_modules/@apollographql/graphql-playground-html/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/packages/server/node_modules/.pnpm/@apollographql+graphql-playground-html@1.6.29/node_modules/@apollographql/node_modules"
