#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/protobufjs/bin/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/protobufjs/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules/.pnpm/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules:/mnt/c/Workspace - Gama/GraphQL/node_modules:/mnt/c/Workspace - Gama/node_modules:/mnt/c/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules/.pnpm/apollo-reporting-protobuf@0.8.0/node_modules/apollo-reporting-protobuf/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules/.pnpm/apollo-reporting-protobuf@0.8.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/protobufjs/bin/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/protobufjs/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules/.pnpm/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules:/mnt/c/Workspace - Gama/GraphQL/node_modules:/mnt/c/Workspace - Gama/node_modules:/mnt/c/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules/.pnpm/apollo-reporting-protobuf@0.8.0/node_modules/apollo-reporting-protobuf/node_modules:/mnt/c/Workspace - Gama/GraphQL/Projeto_aula/node_modules/.pnpm/apollo-reporting-protobuf@0.8.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@apollo+protobufjs@1.2.2/node_modules/@apollo/protobufjs/bin/pbjs" "$@"
else
  exec node  "$basedir/../../../../../@apollo+protobufjs@1.2.2/node_modules/@apollo/protobufjs/bin/pbjs" "$@"
fi
